{
    "name": "ts-template",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "build:client": "webpack --config webpack.client.config.js",
        "build:prod": "NODE_ENV=production concurrently --names \"CLIENT,SERVER\" -c \"bgGreen,bgMagenta\" \"npm run build:client\" \"npm run build:server\"",
        "build:server": "webpack --config webpack.server.config.js",
        "cleanup": "rimraf public app.js tslint.log",
        "dev:client": "npm run build:client -- --watch",
        "dev:server": "npm run build:server -- --watch",
        "predev": "npm run build:server",
        "dev": "concurrently -k --names \"NODEMON,CLIENT,SERVER\" -c \"bgBlue,bgGreen,bgMagenta\" \"nodemon app.js\" \"npm run dev:client\" \"npm run dev:server\"",
        "lint": "tslint --project tsconfig.json --fix --out tslint.log",
        "prestart": "npm run build:prod",
        "start": "node app.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "ejs": "^2.5.7",
        "express": "^4.16.2",
        "jquery": "^3.3.1",
        "morgan": "^1.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.39",
        "@babel/preset-env": "^7.0.0-beta.39",
        "@babel/preset-stage-0": "^7.0.0-beta.39",
        "@chrisaguilar/tslint-config": "^1.3.3",
        "@types/ejs": "^2.5.0",
        "@types/express": "^4.11.1",
        "@types/jquery": "^3.3.0",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.4.0",
        "babel-loader": "^8.0.0-beta.0",
        "cache-loader": "^1.2.0",
        "concurrently": "^3.5.1",
        "fork-ts-checker-webpack-plugin": "^0.3.0",
        "html-webpack-plugin": "^2.30.1",
        "html-webpack-template": "^6.1.0",
        "nodemon": "^1.14.12",
        "rimraf": "^2.6.2",
        "thread-loader": "^1.1.2",
        "ts-loader": "^3.4.0",
        "ts-node": "^4.1.0",
        "tsconfig-paths-webpack-plugin": "^2.0.0",
        "tslint": "^5.9.1",
        "typescript": "^2.7.1",
        "uglifyjs-webpack-plugin": "^1.1.8",
        "webpack": "^3.10.0",
        "webpack-node-externals": "^1.6.0"
    }
}
